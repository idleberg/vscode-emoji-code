{
    "name": "emoji-code",
    "displayName": "Emoji Code",
    "description": "Snippets to insert escaped Emoji code into a variety of languages",
    "version": "0.8.0",
    "publisher": "idleberg",
    "license": "MIT",
    "author": {
        "name": "Jan T. Sott",
        "url": "http://github.com/idleberg"
    },
    "scripts": {
        "build": "node ./src/build.js",
        "postinstall": "npm run build",
        "test": "vscode-linter",
        "vscode:prepublish": "gulp"
    },
    "keywords": [
        "unicode",
        "emoji",
        "smileys",
        "emoticons"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/idleberg/vscode-emoji-code"
    },
    "homepage": "https://github.com/idleberg/vscode-emoji-code#readme",
    "bugs": {
        "url": "https://github.com/idleberg/vscode-emoji-code/issues"
    },
    "icon": "images/logo.png",
    "galleryBanner": {
        "color": "#f76964",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "blade",
                "path": "./snippets/emoji-html.json"
            },
            {
                "language": "css",
                "path": "./snippets/emoji-css.json"
            },
            {
                "language": "html",
                "path": "./snippets/emoji-html.json"
            },
            {
                "language": "jade",
                "path": "./snippets/emoji-html.json"
            },
            {
                "language": "coffeescript",
                "path": "./snippets/emoji-javascript.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/emoji-javascript.json"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/emoji-html.json"
            },
            {
                "language": "json",
                "path": "./snippets/emoji-javascript.json"
            },
            {
                "language": "livescript",
                "path": "./snippets/emoji-javascript.json"
            },
            {
                "language": "twig",
                "path": "./snippets/emoji-html.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/emoji-javascript.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/emoji-javascript.json"
            },
            {
                "language": "vue-html",
                "path": "./snippets/emoji-javascript.json"
            },
            {
                "language": "markdown",
                "path": "./snippets/emoji-markdown.json"
            },
            {
                "language": "python",
                "path": "./snippets/emoji-python.json"
            },
            {
                "language": "ruby",
                "path": "./snippets/emoji-ruby.json"
            },
            {
                "language": "csharp",
                "path": "./snippets/emoji-csharp.json"
            }
        ]
    },
    "dependencies": {
        "emojibase-data": "^4.0.2"
    },
    "devDependencies": {
        "babel-eslint": "^10.0.3",
        "eslint": "^6.3.0",
        "gulp": "^4.0.2",
        "gulp-raster": "^0.2.0",
        "gulp-rename": "^1.4.0",
        "husky": "^3.0.5",
        "vscode": "^1.1.36",
        "vscode-linter": "^0.2.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run test"
        }
    }
}
